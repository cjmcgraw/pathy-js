{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
        "space-before-function-paren": ["error", "never"],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["warn"]
    }
}
